{
  "name": "caseparser",
  "version": "2.0.3",
  "description": "Convert Strings/Arrays/Objects from a type of case to another one.",
  "author": "Fernando Machado Bernardino <fernandombernardino@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/NandoMB/caseparser#readme",
  "bugs": {
    "url": "https://github.com/NandoMB/caseparser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NandoMB/caseparser.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^18.15.11",
    "@vitest/coverage-istanbul": "^0.29.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "keywords": [
    "caseparser",
    "case parser",
    "parser",
    "convert",
    "camel case",
    "snake case",
    "dash case",
    "pascal case",
    "upper snake case",
    "upper dash case",
    "camelCase",
    "snake_case",
    "dash-case",
    "PascalCase",
    "UPPER_SNAKE_CASE",
    "UPPER-DASH-CASE"
  ]
}
